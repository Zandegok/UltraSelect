<Window x:Class="UltimateSelect.OverlayWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStyle="None"
        AllowsTransparency="True"
        Background="#04FFFFFF"
        WindowState="Maximized"
        Topmost="True"
        ShowInTaskbar="False">
    <Canvas x:Name="SelectionCanvas">
        <Rectangle x:Name="SelectionRectangle" Stroke="Black" StrokeThickness="1" Visibility="Collapsed" />
    </Canvas>
</Window>
