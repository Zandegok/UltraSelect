<Window x:Class="UltimateSelect.Views.OverlayWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStyle="None"
        AllowsTransparency="True"
        Background="#40FFFFFF"
        Topmost="True"
        ShowInTaskbar="False">
    <Canvas x:Name="SelectionCanvas">
        <Rectangle x:Name="SelectionRectangle" Stroke="Blue" StrokeThickness="2" Visibility="Collapsed"/>
    </Canvas>
</Window>
